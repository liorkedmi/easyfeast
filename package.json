{
  "name": "easyfeast",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "pnpm localize",
    "dev": "next dev",
    "dev2": "email dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "extract": "formatjs extract src/**/*.js*  --out-file lang/en.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "compile": "formatjs compile lang/en.json --ast --out-file compiled-lang/en.json",
    "localize": "pnpm extract '**/*.jsx' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' && pnpm compile lang/en.json --ast --out-file compiled-lang/en.json"
  },
